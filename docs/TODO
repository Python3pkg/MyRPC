IMPL:

class Impl {
  void osszead(int a, int b); // top half
  int myrpc_continue_osszead(); // bottom half
}

ONREAD_FINISHED:

proc = new Processor(new Impl())
proc.process_one_async(tr,codec)
if (proc->finished()) // Read failed
   return;

(in Impl: register(op, proc))

ONPROC_FINISHED:

proc->process_continue(); // call Impl->myrpc_continue_osszead(...);
// Read transport wbuf + send it.
// Release proc.

